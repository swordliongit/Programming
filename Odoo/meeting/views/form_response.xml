<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="form_response_1" name="Car Select">
        <t t-call="website.layout">
            <t name="carselect" t-name="transfer.form_response_1">
                <div class="container">
                    <span class="d-none" t-esc="rate"/>
                    <span class="d-none" t-esc="product.name"/>
                    <t t-foreach="product.car_lines" t-as="car">
                        <div class="row my-3">
                            <div class="col-12 card card bg-dark text-white mx-3">
                                <form role="form" action="/create/form-2" method="POST">
                                    <t t-set="from_id" t-value="env['transfer.city'].search([['id','=',form_1_details['from_id']]])"/>
                                    <t t-set="to_id" t-value="env['transfer.city'].search([['id','=',form_1_details['to_id']]])"/>
                                    <t t-set="currency_id" t-value="env['res.currency'].search([['id','=',form_1_details['currency_id']]])"/>
                                    <t t-set="rate" t-value="currency_id.rate"/>
                                    <div class="card-header">
                                        <h2 t-esc="car.car_id.name"/>
                                        <div class="col">
                                            <div class="row">
                                                <div class="col-12">
                                                    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                                        <ol class="carousel-indicators">
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                                                            <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
                                                        </ol>
                                                        <div class="carousel-inner">
                                                            <div class="carousel-item active">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_1/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                            <div class="carousel-item">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_2/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                            <div class="carousel-item">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_3/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                            <div class="carousel-item">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_4/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                            <div class="carousel-item">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_5/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                            <div class="carousel-item">
                                                                <img t-attf-src="/website/image/transfer.car/#{car.car_id.id}/image_6/500x200" class="d-block w-100" alt="..."/>
                                                            </div>
                                                        </div>
                                                        <button class="carousel-control-prev" type="button" data-target="#carouselExampleIndicators" data-slide="prev">
                                                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                            <span class="sr-only">Previous</span>
                                                        </button>
                                                        <button class="carousel-control-next" type="button" data-target="#carouselExampleIndicators" data-slide="next">
                                                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                            <span class="sr-only">Next</span>
                                                        </button>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-lg-8 col-md-12 py-3">
                                                    <div class="d-flex justify-content-start flex-warp">
                                                        <div class="d-flex flex-column align-items-center text-center">
                                                            <i class="fa fa-group fa-2x m-1"></i>
                                                            <span class="small mx-1 mb-3">
                                                                <span t-esc="car.car_id.passenger_cap"></span> Passenger
                                                            </span>
                                                        </div>
                                                        <div class="d-flex flex-column align-items-center text-center">
                                                            <i class="fa fa-briefcase fa-2x m-1"></i>
                                                            <span class="small mx-3 mb-3">
                                                                <span t-esc="car.car_id.luggage_cap"></span> Luggage
                                                            </span>
                                                        </div>
                                                        <div class="d-flex flex-column align-items-center text-center">
                                                            <i class="fa fa-automobile fa-2x m-1"></i>
                                                            <span class="small mx-1 mb-3">Car Type
                                                                <span t-esc="car.car_id.type"></span>
                                                            </span>
                                                        </div>
                                                        <!-- <t t-foreach="car.car_id.extras.filtered(lambda x: x.extra_status == True)" t-as="extra">
                                                                <div class="d-flex flex-column align-items-center text-center">
                                                                    <img src="/website/image/transfer.extra/#{extra.id}/image/42x42" alt="" width="42" class="img-thumbnail"/>
                                                                    <span class="small text-uppercase mx-3 my-3">
                                                                        <span t-esc="extra.name"/>
                                                                    </span>
                                                                </div>
                                                            </t> -->
                                                        <!-- <t t-foreach="car.car_id.extras.filtered(lambda x: x.extra_status == False)" t-as="extra">
                                                                <div class="d-flex flex-column align-items-center text-center">
                                                                    <img src="/website/image/transfer.extra/#{extra.id}/image/42x42" alt="" width="42" class="img-thumbnail"/>
                                                                    <span class="small text-uppercase mx-3 my-3">
                                                                        <span t-esc="extra.name"/>
                                                                    </span>
                                                                </div>
                                                            </t> -->
                                                    </div>
                                                    <div class="tagcloud06">
                                                        <t t-set="free_extra_status_list" t-value="car.car_id.extras.filtered(lambda x: x.extra_status == False)"/>
                                                        <span t-if="free_extra_status_list">
                                                                  Free Service  
                                                                  </span>
                                                        <ul>
                                                            <t t-foreach="car.car_id.extras.filtered(lambda x: x.extra_status == False)" t-as="extra">
                                                            <t t-if="extra.extra_car_show == True">
                                                                <li>
                                                                    <a href="#">
                                                                        <span t-esc="extra.name"/>
                                                                    </a>
                                                                </li>
                                                                </t>
                                                            </t>
                                                        </ul>
                                                        <t t-set="paid_extra_status_list" t-value="car.car_id.extras.filtered(lambda x: x.extra_status == True)"/>
                                                        <span t-if="paid_extra_status_list">
                                                                  Paid Service 
                                                                  </span>
                                                        <ul>
                                                            <t t-foreach="car.car_id.extras.filtered(lambda x: x.extra_status == True)" t-as="extra">
                                                                <li t-if="extra.extra_car_show">
                                                                    <a href="#">
                                                                        <span t-esc="extra.name"/>
                                                                    </a>
                                                                </li>
                                                            </t>
                                                        </ul>
                                                        <!-- <t t-foreach="car.car_id.extras.filtered(lambda x: x.extra_status == True)" t-as="extra">
                                                                Ücretli
                                                                <ul>
                                                                    <li t-esc="extra.name"/>
                                                                </ul>
                                                            </t> -->
                                                    </div>
                                                </div>
                                                <div class="col align-self-center col-lg-4 text-right order-last order-lg-2 order-md-last order-sm-last py-3">
                                                    <div>From
                                                        <span t-esc="from_id.name"/>
                                                    </div>
                                                    <div>To
                                                        <span t-esc="to_id.name"/>
                                                    </div>
                                                    <h3>
                                                        <span class="d-none" t-esc="car.car_price"/>
                                                        <t t-set="last_price" t-value="car.car_price*rate"/>
                                                        <span t-esc="'%.2f'% last_price"/>
                                                        <span t-esc="currency_id.name"/>
                                                    </h3>
                                                    <div class="btn-group-lg pt-3">
                                                        <button class="btn btn-primary btn-group-lg" type="submit">Select Car</button>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-lg-8 col-md-8 col-sm-12 align-self-start text-left order-3 mb-1">
                                                    <div class="module">
                                                        <p class="collapse" id="collapseExample" aria-expanded="false">
                                                            <a role="button" class="collapsed" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
                                                                <span t-esc="car.car_id.description"/>
                                                            </a>
                                                        </p>
                                                    </div>
                                                </div>
                                                <div class="form-group d-none">
                                                    <input type="text" name="product_id" t-att-value="product.id" id="product_id" class="form-control" required="required"/>
                                                    <input type="text" name="car_id" t-att-value="car.car_id.id" id="car_id" class="form-control" required="required"/>
                                                    <input type="text" name="price" t-att-value="car.car_price*rate" id="price" class="form-control" required="required"/>
                                                    <input type="text" name="currency_id" t-att-value="currency_id.name" id="currency_id" class="form-control" required="required"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </t>
                </div>
            </t>
        </t>
    </template>
    <template id="form_response_2" name="Reservation Form">
        <t t-call="website.layout">
            <t t-set="product" t-value="env['product.template'].search([['id','=',form_2_details['product']]])"/>
            <t t-set="car" t-value="env['transfer.car'].search([['id','=',form_2_details['car']]])"/>
            <t t-set="price" t-value="form_2_details['price']"/>
            <t t-set="currency" t-value="env['res.currency'].search([['name','=',form_2_details['currency']]])"/>
            <div class="container">
                <form role="form" action="/create/form-3" method="POST">
                    <div class="row ">
                        <div class="col-md-8 p-3">
                            <div class="card text-white bg-dark">
                                <div class="card-header text-primary">
                                    <h3>Transfer Details</h3>
                                </div>
                                <div class="col">
                                    <div class="row">
                                        <div class="col-12">
                                            <h6 t-esc="product.name"/>
                                        </div>
                                        <div class="form-group col-6">
                                            <label for="">* Pickup Date</label>
                                            <input type="date" name="going_transfer_date" id="going_transfer_date" class="form-control" placeholder="Transfer Tarihi" required="required" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Specify the arrival date.</small>
                                        </div>
                                        <div class="form-group col-6">
                                            <label for="">* Pickup Time</label>
                                            <input type="time" name="going_transfer_time" id="going_transfer_time" class="form-control" placeholder="Transfer Saati" aria-describedby="helpId" required="required"/>
                                            <small id="helpId" class="text-white-50">Enter your arrival time in 24-hour format.</small>
                                        </div>
                                        <div class="form-group col-6">
                                            <label for="">Arrival Flight Code</label>
                                            <input type="text" name="going_flight_no" id="going_flight_no" class="form-control" placeholder="TK0707" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Enter your flight code.</small>
                                        </div>
                                        <div class="form-group col-6 col">
                                            <label for="">Destination (Hotel, Facility Name)</label>
                                            <input type="text" name="going_destination" id="going_destination" class="form-control" placeholder="Ex: Dedeman Hotel" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Specify the destination. For example; Hotel, district or public area etc.</small>
                                        </div>
                                    </div>
                                </div>
                                <div class="col text-right">
                                    <div class="form-group text-right">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" name="return_status" id="return_status" value="checkedValue"/>
                                            Return Transfer
                                        </label>
                                    </div>
                                </div>
                                <div class="col d-none" id="return_div">
                                    <div class="row">
                                        <div class="form-group col-6">
                                            <label for="">* Date of return</label>
                                            <input type="date" name="coming_transfer_date" id="coming_transfer_date" class="form-control" placeholder="" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Specify the arrival date.</small>
                                        </div>
                                        <div class="form-group col-6">
                                            <label for="">* Time you want to be pick up</label>
                                            <input type="time" name="coming_transfer_time" id="coming_transfer_time" class="form-control" placeholder="Dönüş Saati" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Specify the arrival time in 24-hour format.</small>
                                        </div>
                                        <div class="form-group col-6">
                                            <label for="">Return Flight Code</label>
                                            <input type="text" name="coming_flight_no" id="coming_flight_no" class="form-control" placeholder="TK0707" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Enter your flight code.</small>
                                        </div>
                                        <div class="form-group col">
                                            <label for="">Destination (Hotel, Facility Name, Place)</label>
                                            <input type="text" name="coming_destination" id="coming_destination" class="form-control" placeholder="Alınacak Yer" aria-describedby="helpId"/>
                                            <small id="helpId" class="text-white-50">Specify the destination. For example; Hotel, district or public area etc.</small>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer">Please do not leave the (*) starred fields blank.</div>
                            </div>
                            <div class="card bg-dark text-white mt-4">
                                <div class="card-header text-primary">
                                    <h3>Personel Information</h3>
                                </div>
                                <div class="col">
                                    <div class="row">
                                        <div class="form-group col-12 col-md-6">
                                            <label for="">* Name</label>
                                            <input type="text" name="customer_name" id="customer_name" placeholder="Name" class="form-control" required="required"/>
                                        </div>
                                        <div class="form-group col-12 col-md-6">
                                            <label for="">* Surname</label>
                                            <input type="text" name="customer_surname" id="customer_surname" placeholder="Surname" class="form-control" required="required"/>
                                        </div>
                                        <div class="form-group col-12 col-md-6">
                                            <label for="">* E-Mail</label>
                                            <input type="email" name="customer_email" id="customer_email" placeholder="E-mail" class="form-control" required="required"/>
                                            <small id="helpId" class="text-white-50">Your order information will be sent to your e-mail address.</small>
                                        </div>
                                        <div class="form-group col-12 col-md-6">
                                            <label for="">* Phone</label>
                                            <input type="tel" name="customer_phone" id="customer_phone" placeholder="Phone" class="form-control" required="required"/>
                                            <!-- <input type="tel" name="customer_country_code" id="customer_country_code" placeholder="Customer Phone Code" class="form-control" /> -->
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer">Please do not leave the (*) starred fields blank.</div>
                            </div>
                            <div class="card bg-dark text-white mt-4">
                                <div class="card-header text-primary">
                                    <h3>Passenger Information</h3>
                                </div>
                                <div class="col">
                                    <div class="row">
                                        <div class="col-4">
                                            <label for="">Adult</label>
                                            <div class="input-group">
                                                <span class="input-group-btn">
                                                    <button type="button" class="yetiskin-plus btn bg-primary btn-number" data-type="plus" data-field="">
                                                        <span class="fa fa-plus"></span>
                                                    </button>
                                                </span>
                                                <input type="number" name="passenger_number" id="passenger_number" class="form-control input-number" value="0" min="0" max="20"/>
                                                <span class="input-group-btn">
                                                    <button type="button" class="yetiskin-minus btn bg-primary btn-number" data-type="minus" data-field="">
                                                        <span class="fa fa-minus"></span>
                                                    </button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-4 pb-3">
                                            <label for="">Kid</label>
                                            <div class="input-group">
                                                <span class="input-group-btn">
                                                    <button type="button" class="cocuk-plus btn bg-primary btn-number" data-type="plus" data-field="">
                                                        <span class="fa fa-plus"></span>
                                                    </button>
                                                </span>
                                                <input type="number" name="child_number" id="child_number" class="form-control input-number" value="0" min="0" max="20"/>
                                                <span class="input-group-btn">
                                                    <button type="button" class="cocuk-minus btn bg-primary btn-number" data-type="minus" data-field="">
                                                        <span class="fa fa-minus"></span>
                                                    </button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label for="">Baby</label>
                                            <div class="input-group">
                                                <span class="input-group-btn">
                                                    <button type="button" class="bebek-plus btn bg-primary btn-number" data-type="plus" data-field="">
                                                        <span class="fa fa-plus"></span>
                                                    </button>
                                                </span>
                                                <input type="number" name="baby_number" id="baby_number" class="form-control input-number" value="0" min="0" max="20"/>
                                                <span class="input-group-btn">
                                                    <button type="button" class="bebek-minus btn bg-primary btn-number" data-type="minus" data-field="">
                                                        <span class="fa fa-minus"></span>
                                                    </button>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="form-group col-12">
                                            <label for="">Other passenger information</label>
                                            <textarea type="textarea" class="form-control" name="another_passengers" id="another_passengers" placeholder="Name Surname" aria-describedby="helpId" rows="3">
                                            </textarea>
                                            <small id="helpId" class="form-text text-muted">
												Example :
                                                <br />
                                                John Paul (add names to lines)
                                                <br />
                                                Anna Paul
                                            </small>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer">Please do not leave the (*) starred fields blank.</div>
                            </div>
                            <div class="card bg-dark text-white mt-4">
                                <div class="card-header text-primary">
                                    <h3>Our services</h3>
                                </div>
                                <div class="col">
                                    <div class="row">
                                        <t t-foreach="car.extras.filtered(lambda x: x.extra_status == True)" t-as="extra">
                                            <t t-if="extra.extra_reservation_show"/>
                                            <div class="row col-12 py-3">
                                                <div class="col-3">
                                                    <img t-attf-src="/website/image/transfer.extra/#{extra.id}/image/500x500" class="img-rounded" alt="Cinque Terre" width="80" height="80"/>
                                                </div>
                                                <div class="col-6">
                                                    <h4 t-esc="extra.name"/>
                                                    <div class="card-subtitle">Description</div>
                                                    <span class="form-text text-muted">Paid Service</span>
                                                </div>
                                                <div class="col-3 self-align-center align-self-center">
                                                    <div class="form-check">
                                                        <label class="form-check-label btn-lg">
                                                            <input class="form-check-input" name="" id="" type="checkbox" value="checkedValue" aria-label="Text for screen reader"/>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                        <t t-foreach="car.extras.filtered(lambda x: x.extra_status == False)" t-as="extra">
                                            <t t-if="extra.extra_reservation_show"/>
                                            <div class="row py-3 col-12">
                                                <div class="col-3">
                                                    <img t-attf-src="/website/image/transfer.extra/#{extra.id}/image/500x500" class="img-rounded" alt="Cinque Terre" width="80" height="80"/>
                                                </div>
                                                <div class="col-6">
                                                    <h4 t-esc="extra.name"/>
                                                    <div class="card-subtitle">Description</div>
                                                    <span class="form-text text-muted">Free Service</span>
                                                </div>
                                                <div class="col-3 align-self-center">
                                                    <div class="input-group">
                                                        <!-- <span class="input-group-btn">
                                                            <button type="button" class="yetiskin-plus btn bg-primary btn-number" data-type="plus" data-field="">
                                                                <span class="fa fa-plus"></span>
                                                            </button>
                                                        </span> -->
                                                        <input type="number" id="yetiskin" name="yetiskin" class="form-control input-number" value="0" min="0" max="3"/>
                                                        <!-- <span class="input-group-btn">
                                                            <button type="button" class="yetiskin-minus btn bg-primary btn-number" data-type="minus" data-field="">
                                                                <span class="fa fa-minus"></span>
                                                            </button>
                                                        </span> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                        <div class="col-4 pb-3">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-12">
                                            <label for="">Your request and suggestion</label>
                                            <textarea type="textarea" class="form-control" name="special_note" id="special_note" aria-describedby="helpId" placeholder="" rows="3"></textarea>
                                            <small id="helpId" class="form-text text-muted">Enter your request and suggestion.</small>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 p-3">
                            <div class="card sticky-top">
                                <div class="card bg-dark text-white">
                                    <div class="card-header">
                                        <h4 class="card-title  text-primary">Summary
                                        </h4>
                                    </div>
                                    <div>
                                        <table class="table table-borderless table-striped table-dark bg-black-60">
                                            <tbody>
                                                <tr>
                                                    <td colspan="2" class="table-dark">
                                                        <h4>Departure Details</h4>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">From</td>
                                                    <td>
                                                        <span t-esc="product.from_id.name"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">To</td>
                                                    <td>
                                                        <span t-esc="product.to_id.name"/>
                                                    </td>
                                                </tr>
                                                <!-- <tr>
                                                    <td scope="row">Gidelecek Yer </td>
                                                    <td>
                                                        <span t-esc="going_destination"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">Transfer Tarihi </td>
                                                    <td>
                                                        <span t-esc="going_transfer_date"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">Transfer Saati </td>
                                                    <td>
                                                        <t t-esc="going_transfer_time"/>
                                                    </td>
                                                </tr> -->
                                                <tr>
                                                    <td scope="row">Vehicle type</td>
                                                    <td>
                                                        <span t-esc="car.name"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">Vehicle model</td>
                                                    <td>
                                                        <span t-esc="car.type"/>
                                                    </td>
                                                </tr>
                                                <!-- <tr>
                                                    <td scope="row">Uçuş Kodunuz </td>
                                                    <td>
                                                        <span t-esc="coming_flight_no"/>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td colspan="2" class="table-dark pt-5">
                                                        <h4>Dönüş Bilgileri</h4>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td scope="row">Dönüş Tarihi </td>
